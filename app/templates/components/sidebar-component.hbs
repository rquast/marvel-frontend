<div class="sidebar__header-logo">The University of Queensland</div>
<hr>
<div>
  <ul class="sidebar__menu">
    {{#each-in menuItems as |key item|}}
      {{#if item.menu}}
      <li>
        {{#link-to item.route class="menu__link"}}<span class="icon icon-{{item.icon}}"></span>{{item.name}}{{/link-to}}
      </li>
      {{/if}}
    {{/each-in}}
  </ul>
</div>
{{yield}}